{
    "version": 2,
    "builds": [
        {
            "src": "./index.html",
            "use": "@now/static-build"
        }
    ],
    "routes": [
        {
            "src": "/*",
            "dest": "./*",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
